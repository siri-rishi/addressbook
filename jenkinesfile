pipeline {
      agent any
           stages {
               stage("Build"){
                          steps{
                               script{
                             echo "Building the code"
                     }
                                }
                    }
                    stage("Compile"){
                                  steps{
                                      script{
                                       echo  "Compiling the code"
                       }
                  } 
                  }
            }
            }
  
